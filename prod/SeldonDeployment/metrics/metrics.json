{
    "kind": "SeldonDeployment",
    "apiVersion": "machinelearning.seldon.io/v1",
    "metadata": {
        "name": "metrics",
        "namespace": "prod",
        "creationTimestamp": null,
        "labels": {
            "fluentd": "true"
        }
    },
    "spec": {
        "name": "metrics",
        "predictors": [
            {
                "name": "default",
                "graph": {
                    "name": "metrics-container",
                    "implementation": "SKLEARN_SERVER",
                    "modelUri": "gs://seldon-models/sklearn/iris",
                    "logger": {
                        "mode": "all"
                    }
                },
                "componentSpecs": [
                    {
                        "metadata": {
                            "creationTimestamp": "2021-04-19T17:37:15Z"
                        },
                        "spec": {
                            "containers": [
                                {
                                    "name": "metrics-container",
                                    "resources": {
                                        "limits": {
                                            "cpu": "1",
                                            "memory": "1Gi"
                                        },
                                        "requests": {
                                            "cpu": "100m",
                                            "memory": "1Gi"
                                        }
                                    }
                                }
                            ]
                        },
                        "hpaSpec": {
                            "minReplicas": 1,
                            "maxReplicas": 2,
                            "metrics": [
                                {
                                    "type": "Resource",
                                    "resource": {
                                        "name": "cpu",
                                        "targetAverageUtilization": 10
                                    }
                                }
                            ]
                        }
                    }
                ],
                "replicas": 1,
                "engineResources": {},
                "svcOrchSpec": {},
                "traffic": 100
            }
        ],
        "annotations": {
            "seldon.io/engine-seldon-log-messages-externally": "true"
        },
        "protocol": "seldon"
    },
    "status": {}
}